    <script type="text/javascript">
        $(document).ready(function(){
            //判斷是否為觸控裝置
            function isMobile() {
                try{ document.createEvent("TouchEvent"); return true; }
                catch(e){ return false;}
            }
            if(!isMobile()){
                $('a').addClass('no-touch');
            }


            var slidView = 0,
                space = 0;

            function onResizeWindow (){
                var winH = $(window).height(),
                    winW = $(window).width(),
                    cardH =  $('.CC-card-list').height(),
                    UrlH = 100;

                $('.card-Welcome[data-style="1"]').css('height',winH);

                if(winW >= 1366 && winH >= 1024 - UrlH){
                    slidView = 3.5
                    space = 80
                }else if(winW >= 1366 && winH >= 768 - UrlH){
                    slidView = 4.5
                    space = 50
                }else if(winW >= 1024 && winH >= 1366 - UrlH){
                    slidView = 2.2
                    space = 80
                }else if(winW >= 1024 && winH >= 768){
                    slidView = 3.2
                    space = 80
                }else if(winW > 1024 && winH < 768){
                    slidView = 4.5
                    space = 80
                }else if(winW >=768 && winH >= 1024 - UrlH){
                    slidView = 2.2
                    space = 50
                }else if(winW <= 375){
                    slidView = 1.6
                }else if(winH > winW - 200){
                    slidView = 3.2
                    space = 50
                }else{
                    slidView = 4.5
                    space = 50
                }
                if($(window).width()< 580){
                    var mainMenuW = $('.main-menu').width();
                    $('.menu').css('width',mainMenuW);
                    $('.menu').transform('translate3d( -500px, 0px, 0px)');
                    $('.header-logo').addClass('menu-toggle');
                    $(".menu-toggle").off().on("click", function() {
                    var $this = $(this);
                    
                    if ($this.hasClass("toggle-active")) {
                        $this.removeClass("toggle-active");
                        $(".menu").removeClass("menu-active").transform('translate3d( -500px, 0px, 0px)');
                        
                    } else {
                        $this.addClass("toggle-active");
                        $(".menu").addClass("menu-active").transform('translate3d( 0, 0px, 0px)');
                        return false;
                    }
                    });
                }else{
                    $('.menu').transform('translate3d( 0, 0px, 0px)').removeClass("menu-active");
                    $('.header-logo').removeClass('menu-toggle toggle-active');
                    $('.header-logo').off();
                }
                if($(window).height()< 739){
                    $('.card-Welcome[data-style="1"] .svg-container').addClass('mobile');
                }
                var mySwiper = new Swiper('.CC-card-list .swiper-container', {
                    slidesPerView: 4.1,
                    speed: 800,
                    nextButton: '.swiper-button-next',
                    prevButton: '.swiper-button-prev',
                    spaceBetween: 130,
                    slidesOffsetBefore: 300,
                    slidesOffsetAfter: 320,
                    mousewheelControl : true,
                    breakpoints: {
                    1440:{
                        slidesPerView: slidView,
                        spaceBetween: space,
                    },
                    1280:{
                        slidesPerView: slidView,
                        spaceBetween: space,
                    },
                    1179:{
                        slidesPerView: slidView,
                        spaceBetween: space,
                    },
                    1024:{
                        slidesPerView: slidView,
                        spaceBetween: space,
                        slidesOffsetBefore: 250
                    },
                    800:{
                        slidesPerView: slidView,
                        spaceBetween: space,
                        slidesOffsetBefore: 250
                    },
                    580: {
                        slidesPerView: 1.6,
                        spaceBetween: 50,
                        slidesOffsetBefore: 80,
                        slidesOffsetAfter: 80,
                    },
                    375:{
                        slidesPerView: slidView,
                        spaceBetween: 50,
                        slidesOffsetBefore: 50,
                        slidesOffsetAfter: 80,
                    }
                    },
                    fadeEffect: {
                        crossFade: true
                    },
                    onSlideChangeStart: function(swiper){
                        //過渡時觸發事件
                        var mainMenuW = $('.main-menu').width();
                        $('.menu').css('width',mainMenuW);

                        if(swiper.activeIndex >= 1 || $(window).width()< 580){
                            $('.menu').removeClass("menu-active").transform('translate3d( -500px, 0px, 0px)');
                            $('.header-logo').addClass('menu-toggle').removeClass('toggle-active');
                            $(".menu-toggle").off().on("click", function() {
                            var $this = $(this);
                            
                            if ($this.hasClass("toggle-active")) {
                                $this.removeClass("toggle-active");
                                $(".menu").removeClass("menu-active").transform('translate3d( -500px, 0px, 0px)');
                                
                            } else {
                                $this.addClass("toggle-active");
                                $(".menu").addClass("menu-active").transform('translate3d( 0, 0px, 0px)');
                                return false;
                            }
                            });
                        }else{
                            $('.menu').transform('translate3d( 0, 0px, 0px)').removeClass("menu-active");
                            $('.header-logo').removeClass('menu-toggle toggle-active');
                            $('.header-logo').off();
                        }
                    },
                    onReachEnd: function(swiper){
                        //過度到最後一個觸發事件
                        $('.swiper-button-next').one("click", function(){
                            swiper.slideTo(0, 1000, true);
                        });
                    }
                });
            }

            onResizeWindow()
            
            
            $(".card-Welcome").on("click", function () {
                if ($('.header-logo').hasClass("toggle-active")) {
                    $('.menu').removeClass("menu-active").transform('translate3d( -500px, 0px, 0px)');
                    $('.header-logo').addClass('menu-toggle').removeClass('toggle-active');
                }
            });
            
            window.addEventListener('resize', function () {
                let timer;
                timer = window.setTimeout(function () {
                    onResizeWindow();
                }, 100); 
            });
        });
    </script>
