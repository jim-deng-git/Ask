                                <div class="constant">
                                    <div>
                                        <div class="input-field">
                                            <input type="file" name="fFavicon" id="fFavicon">
                                            @{
                                                string IconStr = "";
                                                if (!string.IsNullOrEmpty(Model.Favicon))
                                                {
                                                    WorkV3.Models.ImageModel iconImgModel = new WorkV3.Models.ImageModel();
